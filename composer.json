{
    "name": "computan/notification",
    "description": "this is private package to use for logging and emailing to developer and client",
    "license": "MIT",
    "authors": [{
        "name": "Tameem Ahmad Shahzad",
        "email": "tshahzad@computan.net"

    }],
    "homepage": "https://github.com/computan/notification",
    "keywords": ["Laravel", "Notification"],
    "require": {
        "php": ">=7.0.0"
    },
    "suggest": {
        "graylog2/gelf-php": "Required for Gelf/Graylog logging",
        "predis/predis": "Required for Redis logging"
    },
    "autoload": {
        "psr-4": {
            "Computan\\LaravelCustomLog\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Computan\\LaravelCustomLog\\LaravelCustomLogServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}